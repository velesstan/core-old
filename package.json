{
  "name": "velesstan-core",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "docker:build": "docker build -t velesstan/core . && docker push velesstan/core",
    "docker:mongodb": "docker-compose up -d database",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run docker:mongodb && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "ncu": "npm-check-updates"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.10",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.10",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^7.2.2",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.10",
    "@nestjs/swagger": "^4.7.12",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.4",
    "handlebars": "^4.7.6",
    "joi": "^17.3.0",
    "mongoose": "^5.11.14",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "puppeteer": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.6",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/passport": "^1.0.5",
    "@types/passport-jwt": "^3.0.4",
    "@types/puppeteer": "^5.4.2",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "mongodb-memory-server": "^6.9.3",
    "npm-check-updates": "^11.1.1",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
